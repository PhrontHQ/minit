{
    "{{name}}": {
        "object": "data/descriptors/{{name}}.mjson"
    },
    "{{exportedName}}Mapping": {
        "object": "data/mapping/{{name}}-mapping.mjson"
    },
    "root": {
        "prototype": "logic/service/{{name}}-service",
        "properties": {
            "name": "{{exportedName}}Service",
            "types": [
                 {"@": "{{name}}"}
             ],
            "mappings": [
                 {"@": "{{exportedName}}Mapping"}
            ]
        }
    }
}

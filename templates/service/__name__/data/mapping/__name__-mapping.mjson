{
    "root": {
        "prototype": "montage/data/service/expression-data-mapping",
        "values": {
            "rawDataPrimaryKeys":["id"],
            "objectDescriptor": {"@": "{{exportedName}}ObjectDescriptor"},
            "objectMapping": {
                "rules": {
                    "id": {"<->": "id"},
                    "subject": {"<->": "subject"},
                    "text": {"<->": "text"},
                    "created": {"<->": "created"},
                    "updated": {"<->": "updated"}
                }
            },
            "requisitePropertyNames": [
                "id", "subject", "text"
            ]
        }
    },
    "{{exportedName}}ObjectDescriptor": {
        "object": "data/descriptors/{{name}}.mjson"
    }
}
